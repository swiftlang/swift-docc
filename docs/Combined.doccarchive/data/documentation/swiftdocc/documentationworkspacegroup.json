  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationWorkspace",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " manages a list of data providers that discover, register, and provide data access to documentation bundles. The data provider protocol ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationWorkspaceDataProvider",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " does not make any assumptions regarding the documentation storage medium as long as the provider can identify those resources by their ",
              "type" : "text"
            },
            {
              "code" : "URL",
              "type" : "codeVoice"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "A common case is to use the pre-defined ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/LocalFileSystemDataProvider",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " that loads any documentation bundles found in a given directory on disk:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let workspace = DocumentationWorkspace()",
            "let dataProvider = try LocalFileSystemDataProvider(rootURL: sourceDirectoryURL)",
            "try workspace.registerProvider(dataProvider)",
            "",
            "guard let firstBundle = workspace.bundles.values.first else {",
            "  fatalError(\"No documentation bundle found\")",
            "}",
            "",
            "print(\"A bundle with ID: \\(firstBundle.identifier)\")",
            "",
            "print(\"Symbol graph files:\")",
            "print(firstBundle.symbolGraphURLs)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "anchor" : "Bundle-Contents",
          "level" : 3,
          "text" : "Bundle Contents",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationBundle",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " offers the information needed to load its contents into memory for processing. A bundle is uniquely identified by its ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/BundleIdentifier",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " identifier.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use the bundle data to load symbol graphs, markup files, assets like images or videos, and bundle metadata.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "bundle.miscResourceURLs",
            "    .filter { $0.lastPathComponent.hasSuffix(\".zip\") }",
            "    .forEach {",
            "      print(\"Download archive: \\($0.lastPathComponent)\")",
            "    }"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
  ],
