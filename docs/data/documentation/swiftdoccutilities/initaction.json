    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Generates a DocC catalog from one of the two available templates:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "articleOnly",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ": A template designed for authoring article-only reference documentation.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "tutorial",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : ": A template designed for authoring tutorials, consisting of a catalog that contains a table of contents and a chapter.",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "ommit",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "Use ",
                      "type" : "text"
                    },
                    {
                      "code" : "docc init",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " to quickly start authoring your content without having to manually add boilerplate content to get the DocC documentation catalog working.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Templates",
          "level" : 2,
          "text" : "Templates",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "There are two available templates to start from, depending on the type of content the author wants to write.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Article-Only-Template",
          "level" : 3,
          "text" : "Article-Only Template",
          "type" : "heading"
        },
        {
          "anchor" : "ArticleOnlymd",
          "level" : 6,
          "text" : "ArticleOnly.md",
          "type" : "heading"
        },
        {
          "code" : [
            "# ArticleOnly Template",
            "",
            "<!--- Metadata configuration to make appear this documentation page as a top-level page -->",
            "",
            "@Metadata {",
            "  @TechnologyRoot",
            "}",
            "",
            "Add a single sentence or sentence fragment, which DocC uses as the page’s abstract or summary.",
            "",
            "## Overview",
            "",
            "Add one or more paragraphs that introduce your content overview."
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "With the structure:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "⎯ ArticleOnly.docc",
            "    ├ ArticleOnly.md",
            "    ⎣ Resources"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "anchor" : "Tutorial-Template",
          "level" : 3,
          "text" : "Tutorial Template",
          "type" : "heading"
        },
        {
          "anchor" : "table-of-contentstutorial",
          "level" : 6,
          "text" : "table-of-contents.tutorial",
          "type" : "heading"
        },
        {
          "code" : [
            "@Tutorials(name: \"\\(title)\") {",
            "    @Intro(title: \"Tutorial Introduction\") {",
            "        Add one or more paragraphs that introduce your tutorial.",
            "    }",
            "    @Chapter(name: \"Chapter Name\") {",
            "        @Image(source: \"add-your-chapter-image-filename-here.jpg\", alt: \"Add an accessible description for your image here.\")",
            "        @TutorialReference(tutorial: \"doc:page-01\")",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "anchor" : "Chapter01page-01tutorial",
          "level" : 6,
          "text" : "Chapter01\/page-01.tutorial",
          "type" : "heading"
        },
        {
          "code" : [
            "@Tutorial() {",
            "    @Intro(title: \"Tutorial Page Title\") {",
            "        Add one paragraph that introduce your tutorial.",
            "    }",
            "    @Section(title: \"Section Name\") {",
            "        @ContentAndMedia {",
            "            Add text that introduces the tasks that the reader needs to follow.",
            "            @Image(source: \"add-your-section-image-filename-here.jpg\", alt: \"Add an accessible description for your image here.\")",
            "        }",
            "        @Steps {",
            "            @Step {",
            "                This is a step with code.",
            "                @Code(name: \"\", file: \"\")",
            "            }",
            "            @Step {",
            "                This is a step with an image.",
            "                @Image(source: \"\", alt: \"\")",
            "            }",
            "        }",
            "    }",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "With the structure:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "⎯ Tutorial.docc",
            "    ├ table-of-contents.tutorial",
            "    ├ Chapter01",
            "    ⎜   ├ page-01.tutorial",
            "    ⎜   ⎣ Resources",
            "    ⎣ Resources"
          ],
          "syntax" : null,
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
