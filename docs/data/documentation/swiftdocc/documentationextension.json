        },
        {
          "inlineContent" : [
            {
              "text" : "By default, content from the documentation-extension file is added after the content from the in-source documentation for that symbol.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "You get this default behavior in two cases:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "when the documentation-extension file doesn’t have a ",
                      "type" : "text"
                    },
                    {
                      "code" : "DocumentationExtension",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " directive",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "when the ",
                      "type" : "text"
                    },
                    {
                      "code" : "DocumentationExtension",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " directive explicitly specifies the ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationExtension\/Behavior-swift.enum\/append",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : " behavior",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "inlineContent" : [
            {
              "text" : "The other merge behavior completely replaces the content from the in-source documentation for that symbol with the content from the documentation-extension file. To get this behavior, specify ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationExtension\/Behavior-swift.enum\/override",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " as the merge behavior.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@Metadata {",
            "   @DocumentationExtension(mergeBehavior: override)",
            "}"
          ],
          "syntax" : null,
          "type" : "codeListing"
        },
        {
