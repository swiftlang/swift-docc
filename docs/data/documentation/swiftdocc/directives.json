      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Besides having a custom init to initialize all required properties, most directives are easily created out of markup source. Each directive has a name, expected parameter list, and content expectations.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "For example ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/ContentAndMedia",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " requires at least one ",
              "type" : "text"
            },
            {
              "code" : "Image",
              "type" : "codeVoice"
            },
            {
              "text" : " or ",
              "type" : "text"
            },
            {
              "code" : "Video",
              "type" : "codeVoice"
            },
            {
              "text" : " directive in its enclosed content:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "@ContentAndMedia {",
            "    Add the `EmailValidator` code.",
            "         ",
            "    @Image(source: code.png,",
            "           alt: \"EmailValidator(string: email.stringValue).validate()\")",
            "}"
          ],
          "syntax" : "markdown",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
