  },
  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "The documentation context generally manages the in-memory documentation including:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Analyzing bundle file contents and converting to semantic models.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Managing a graph of documentation nodes (a single node representing one documentation topic).",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Processing assets like media files or download archives.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Resolving links to external documentation via ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/ExternalReferenceResolver",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : " and resolving external symbols via ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/ExternalSymbolResolver",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : ".",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Random access to documentation data including walking the graph and path finding.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Creating-a-Context",
          "level" : 3,
          "text" : "Creating a Context",
          "type" : "heading"
        },
        {
          "code" : [
            "let workspace = DocumentationWorkspace()",
            "let context = try DocumentationContext(dataProvider: workspace)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "During initialization the context will inspect the available bundles in the workspace and load any symbol graph files and markup files.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Accessing-Documentation",
          "level" : 3,
          "text" : "Accessing Documentation",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationContext\/entity(with:)",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " to access a documentation node by its topic reference:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let reference = ResolvedTopicReference(",
            "    bundleIdentifier: \"com.mybundle\",",
            "    path: \"\/documentation\/ValidationKit\/EmailValidator\",",
            "    fragment: nil,",
            "    sourceLanguage: .swift)",
            "let node = try context.entity(with: reference)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "To find out the location of the source file for a given documentation node use:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let sourceFileURL = try context.documentURL(for: reference)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "And finally to print all known paths in the context:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "context.knownIdentifiers.forEach({ print($0) })"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
