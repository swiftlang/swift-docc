    "title" : "Documentation Context"
  },
  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A documentation context is the the in-memory representation of a “unit” of documentation (for example a module, package, or technology).",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "The context is generally responsible for:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Analyzing bundle file contents and converting to semantic models.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Managing a graph of documentation nodes (a single node representing one documentation topic).",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Processing assets like media files or download archives.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Resolving links to external documentation sources via ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/ExternalDocumentationSource",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : " and resolving external symbols via ",
                      "type" : "text"
                    },
                    {
                      "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/GlobalExternalSymbolResolver",
                      "isActive" : true,
                      "type" : "reference"
                    },
                    {
                      "text" : ".",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Random access to documentation data including walking the graph and path finding.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Creating-a-Context",
          "level" : 3,
          "text" : "Creating a Context",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use ",
              "type" : "text"
            },
            {
              "code" : "DocumentationContext\/init(bundle:dataProvider:diagnosticEngine:configuration:)",
              "type" : "codeVoice"
            },
            {
              "text" : " to create a context for a given bundle:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let inputsProvider = DocumentationContext.InputsProvider()",
            "let (bundle, dataProvider) = try inputsProvider.inputsAndDataProvider(",
            "    startingPoint: catalogURL, ",
            "    options: bundleDiscoveryOptions",
            ")",
            "",
            "let context = try DocumentationContext(bundle: bundle, dataProvider: dataProvider)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "anchor" : "Accessing-Documentation",
          "level" : 3,
          "text" : "Accessing Documentation",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Use ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationContext\/entity(with:)",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : " to access a documentation node by its topic reference:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let reference = ResolvedTopicReference(",
            "    bundleID: \"com.mybundle\",",
            "    path: \"\/documentation\/ValidationKit\/EmailValidator\",",
            "    fragment: nil,",
            "    sourceLanguage: .swift)",
            "let node = try context.entity(with: reference)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "To find out the location of the source file for a given documentation node use:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let sourceFileURL = try context.documentURL(for: reference)"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
