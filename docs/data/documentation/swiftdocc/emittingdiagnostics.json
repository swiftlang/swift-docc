  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "When running a compilation from the command line ",
              "type" : "text"
            },
            {
              "code" : "docc",
              "type" : "codeVoice"
            },
            {
              "text" : " prints all diagnostics as output. Emitting rich diagnostics is important for integrating the compilation output with automation tools or development environments.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The diagnostics emitted during discovery, loading, and processing documentation are available via the context ",
              "type" : "text"
            },
            {
              "identifier" : "doc:\/\/SwiftDocC\/documentation\/SwiftDocC\/DocumentationContext\/problems",
              "isActive" : true,
              "type" : "reference"
            },
            {
              "text" : ".",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Returning a diagnostic when a callback or a method expects it is straight-forward:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "let explanation = \"Level-1 headings are reserved for titles.\"",
            "let diagnostic = Diagnostic(",
            "    source: sourceURL, ",
            "    severity: .warning, ",
            "    range: heading.range, ",
            "    identifier: \"org.swift.docc.InvalidAdditionalTitle\", ",
            "    summary: \"Invalid use of level-1 heading.\", ",
            "    explanation: explanation)",
            "",
            "return Problem(diagnostic: diagnostic, possibleSolutions: [])"
          ],
          "syntax" : "swift",
          "type" : "codeListing"
        }
      ],
      "kind" : "content"
    }
