            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and DocC encounters a ",
              "type" : "text"
            },
            {
              "code" : "doc:\/\/your.resolver.id\/path\/to\/some-page#some-fragment",
              "type" : "codeVoice"
            },
            {
              "text" : " link in any documentation content, DocC sends the ",
              "type" : "text"
            },
            {
              "code" : "\"\/path\/to\/some-page#some-fragment\"",
              "type" : "codeVoice"
            },
            {
              "text" : " link to your resolver.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
